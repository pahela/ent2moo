package entregable2;

/**
 *
 * @author pablo
 */
public class EsContacto extends Publicacion {
   private Persona elContacto;
   
   
   public EsContacto(long fecha, Persona autor, Persona elContacto){
       super(fecha,autor);
       this.elContacto=elContacto;
       
   }
   public String toString(){
        return this.autor+" es ahora contacto de "+this.elContacto;
        
    }
   
   public String getCabecera(){
        
        return this.autor.getNombre()+" es ahora contacto de "+autor.getNombre()+"("+fecha+")";
         
    }
}
