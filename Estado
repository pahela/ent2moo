package entregable2;

/**
 *
 * @author pablo
 */
public class Estado extends Publicacion {
    private String texto;
    
    public Estado(long fecha, Persona autor,String texto){
       super(fecha,autor);
       this.texto=texto;
       
   }
    public String toString(){
        return ">"+this.texto;
        
    }
}
